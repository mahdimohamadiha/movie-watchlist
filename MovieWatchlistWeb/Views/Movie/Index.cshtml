@model IEnumerable<Movie>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Movie</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Movie Name
            </th>
            <th>
                Genre
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.Genre
                </td>
            </tr>
        }
    </tbody>
</table>

